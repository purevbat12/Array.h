#include "array.h"
#include <runtime.h>
int main(void){
	cStart(creation);
	createArr(a, 'i', 3, i(1, 2, 3));
	createArr(b, 'i', 3, i(4, 5, 6));
	createArr(c, 'i', 3, i(7, 8, 9));
